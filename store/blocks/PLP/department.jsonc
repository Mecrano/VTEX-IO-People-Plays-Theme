{
	"search-result-layout#department": {
		"blocks": [
			"search-result-layout.desktop#department",
			"search-result-layout.mobile#department",
			"search-not-found-layout"
		],
		"props": {
			"blockClass": "plp--main"
		}
	},
	"search-result-layout.desktop#department": {
		"props": {
			"fullWidth": true,
			"preventRouteChange": true,
			"showFacetQuantity": true,
			"brands": true,
			"priceRange": true,
			"blockClass": "desktop-plp--main-container"
		},
		"children": [
			"flex-layout.row#department-main"
		]
	},
		"search-result-layout.mobile#department": {
		"props": {
			"fullWidth": true,
			"preventRouteChange": true,
			"showFacetQuantity": true,
			"brands": true,
			"priceRange": true,
			"blockClass": "mobile-plp--main-container"
		},
		"children": [
			"flex-layout.row#department-main"
		]
	},
	"flex-layout.row#department-main": {
		"props": {
			"blockClass": "department-main-container-row",
			"fullWidth": true,
			"preserveLayoutOnMobile": true
		},
		"children": [
			"flex-layout.col#department-main"
		]
	},
	"flex-layout.col#department-main": {
		"children": [
			"breadcrumb.search",
			"responsive-layout.desktop#department-image",
			"responsive-layout.tablet#department-image",
			"responsive-layout.phone#department-image",
      "flex-layout.row#departament-slider-one",
			"flex-layout.row#department-two-banners",
			/* "list-context.info-card-list#promociones", */
      "flex-layout.row#encuentra-departament",
			"flex-layout.row#department-newsletter-banner",
      "flex-layout.row#departament-slider-two"
		],
		"props": {
			"blockClass": "department-main-container-col"
		}
	},

/* slider */
"flex-layout.row#departament-slider-one": {
  "title": "block 4",
  "props":{
    "blockClass": "home-seccion-three"
  },
  "children":["flex-layout.row#titles-promociones-departamento", "flex-layout.row#shelf1-departament" ]
},
"flex-layout.row#titles-promociones-departamento":{
  "children":["rich-text#promociones-departament"]
},
"rich-text#promociones-departament": {
  "title": "title",
  "props": {
    "textAlignment": "LEFT",
    "textPosition": "LEFT",
    "text": "Promociones",
    "blockClass": "promocion"
  }
},
"flex-layout.row#shelf1-departament": {
  "title": "contenido vitrina",
  "children": ["list-context.product-list#productSlider-departament"],
  "props": {"blockClass": "shelfComplete"}
},
"list-context.product-list#productSlider-departament": {
  "blocks": ["product-summary.shelf#plp"],
  "children": ["slider-layout#products-departament"],
  "props": {
    "category": "9285",
    "orderBy": "OrderByTopSaleDESC"
  }
},
"slider-layout#products-departament": {
  "props": {
    "itemsPerPage": {
      "desktop": 4,
      "tablet": 3,
      "phone": 1
    },
    "infinite": false,
    "fullWidth": true,
    "showPaginationDots": "always",
    "showNavigationArrows": "desktopOnly",
    "blockClass": "shelfStyle1"
  }
},
/* slider */

